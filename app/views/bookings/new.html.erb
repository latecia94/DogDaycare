<h1>Dog Booking Form</h1>

<%= form_with model: @booking, local: true do |f| %>
  <%= f.label :dogs %>
  <%= f.collection_select :dogs_id, current_user.dogs.order(:name), :id, :name %><br>

  <%= f.label :locations %>
  <%= f.collection_select :locations_id, Location.order(:name), :id, :name %><br>

  <%= f.label :date %>
  <%= f.date_select :date, :order => [:day, :month, :year] %><br>

  <%= f.submit 'Submit' %>
<% end %>
